  "version": "0.2.2",
  "authors": [
    {
      "name": "Pablo del Vecchio",
      "email": "dvpablo@yahoo.com"
    }
  ],
  "extra": {
    "branch-alias": {
      "dev-master": "0.2.x-dev"
    }
  },
  "require": {
    "php": ">=5.6.0",
    "aws/aws-sdk-php": ">=3.63,<4.0",
    "elasticsearch/elasticsearch": ">=5.3,<6.0",
    "mongodb/mongodb": ">=1.3,<2.0"
  },
  "autoload": {
    "psr-4": {
      "Picnic\\": "src/"
    }
  }
